<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<!-- Programs -->
	<servlet>
		<servlet-name>Programs</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.ProgramsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Programs</servlet-name>
		<url-pattern>/programs</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>NewProgramForm</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.NewProgramFormServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NewProgramForm</servlet-name>
		<url-pattern>/programs/new</url-pattern>
	</servlet-mapping>
	
	<!-- Students -->
	<servlet>
		<servlet-name>Students</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.StudentsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Students</servlet-name>
		<url-pattern>/students</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CreateStudentForm</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.CreateStudentFormServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateStudentForm</servlet-name>
		<url-pattern>/students/new</url-pattern>
	</servlet-mapping>
	
	<!-- Log-in -->
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LogIn</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.LogInServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogIn</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Homepage</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.HomepageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Homepage</servlet-name>
		<url-pattern>/homepage</url-pattern>
	</servlet-mapping>
	
	<!-- ClassList/Enrolling -->
	<servlet>
		<servlet-name>SingleProgram</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.SingleProgramServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleProgram</servlet-name>
		<url-pattern>/programs/single</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EnrollStudent</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.EnrollStudentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EnrollStudent</servlet-name>
		<url-pattern>/programs/enroll</url-pattern>
	</servlet-mapping>
	
	<!-- CookieHelperServlets -->
	<servlet>
		<servlet-name>User</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>User</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LogOut</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.LogOutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogOut</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<!-- Treasury -->
	<servlet>
		<servlet-name>Treasury</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.TreasuryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Treasury</servlet-name>
		<url-pattern>/treasury</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ProgramTreasury</servlet-name>
		<servlet-class>edu.uwm.cs361.fantastic_five.training_tracker.ProgramTreasuryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProgramTreasury</servlet-name>
		<url-pattern>/treasury/program</url-pattern>
	</servlet-mapping>
	
	<!-- index -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
